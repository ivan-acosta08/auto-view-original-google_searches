{
  "manifest_version": 3,
  "name": "Google: Auto 'See original'",
  "version": "1.0.1",
  "description": "Avoids translated views in Google; automatically clicks on 'View original'.",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
  "https://www.google.com/search*"
],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}